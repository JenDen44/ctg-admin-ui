{
  "name": "ctg-admin-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "preview": "vite preview",
    "depcruise": "depcruise --config dependency-cruiser.cjs src",
    "depcruise:graph": "rm -rf depcruise-report && mkdir -p depcruise-report && depcruise --config dependency-cruiser.cjs src --output-type dot | dot -T svg > depcruise-report/dependency-graph.svg",
    "depcruise:cycles": "rm -rf depcruise-report && mkdir -p depcruise-report && depcruise --config dependency-cruiser.cjs src --output-type dot --focus circular | dot -T svg > depcruise-report/cycles.svg"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.8",
    "@hookform/resolvers": "^5.2.2",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@tanstack/react-query": "^5.90.2",
    "axios": "^1.12.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.64.0",
    "react-router": "^7.9.3",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@tanstack/eslint-plugin-query": "^5.91.0",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vitejs/plugin-react": "^5.0.4",
    "dependency-cruiser": "^17.0.2",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.1.1",
    "eslint-plugin-react-refresh": "^0.4.23",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.9",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
